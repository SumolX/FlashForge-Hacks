# Returns success on substring match 
utils_has_string() {
    str="$1"
    sub="$2"
    if [ "${str#*"$sub"}" != "$str" ]; then
        return 0
    else
        return 1
    fi
}

# Transfer binary file to client
utils_send_binary_file() {
    echo "content-type: application/x-download"
    echo "content-disposition: attachment; filename=$2"
    echo ""
    
    cat $1/$2
}

